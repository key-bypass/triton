<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/color_1b1a28"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:background="@color/color_2a2b40"
        android:layout_width="match_parent"
        android:layout_height="200px"
        android:layout_marginTop="10px"/>
    <LinearLayout
        android:layout_gravity="center"
        android:orientation="vertical"
        android:id="@+id/ll_foreign"
        android:background="@drawable/bg_4b4d62_r10"
        android:padding="15px"
        android:layout_width="900px"
        android:layout_height="wrap_content">
        <TextView
            android:textSize="20px"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Manufactured by Hunan Senpuluo Technology Co., Ltd\10Distributed by Lock Labs Inc.\10Las Vegas, NV\10For service and repairs, please contact the Lock Labs Warranty and Repair Center."/>
        <LinearLayout
            android:orientation="horizontal"
            android:visibility="visible"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10px"
            android:layout_marginBottom="20px">
            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:textSize="24px"
                    android:textColor="@color/white"
                    android:gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20px"
                    android:layout_marginTop="5px"
                    android:text="@string/phone"
                    android:drawableTop="@drawable/phone"/>
                <TextView
                    android:textSize="16px"
                    android:textColor="@color/white"
                    android:gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="40px"
                    android:layout_marginTop="20px"
                    android:layout_marginRight="20px"
                    android:text="(855) 562-5522 ext. 4005"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:textSize="24px"
                    android:textColor="@color/white"
                    android:gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20px"
                    android:layout_marginTop="5px"
                    android:text="@string/e_mail"
                    android:drawableTop="@drawable/email"/>
                <TextView
                    android:textSize="16px"
                    android:textColor="@color/white"
                    android:gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="40px"
                    android:layout_marginTop="20px"
                    android:layout_marginRight="20px"
                    android:text="support@lock-labs.com"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:textSize="24px"
                    android:textColor="@color/white"
                    android:gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20px"
                    android:layout_marginTop="5px"
                    android:text="Website"
                    android:drawableTop="@drawable/website"/>
                <TextView
                    android:textSize="16px"
                    android:textColor="@color/white"
                    android:gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="40px"
                    android:layout_marginTop="20px"
                    android:layout_marginRight="20px"
                    android:text="www.tritonkeymachine.com"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>
